{
  "name": "icon-links",
  "version": "0.1.3",
  "description": "This is similar to Quick links but saves the data in a SharePoint list. You can use the links across multiple pages & sites without having to recreate them.",
  "main": "src/index.ts",
  "scripts": {
    "all": "npm run build && npm run prod && npm run spfx",
    "build": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "spfx": "cd spfx && npm run package",
    "test": "npm run build && cd spfx && gulp serve --nobrowser",
    "increment-version": "node ./incrementVersion.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spsprinkles/icon-links.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spsprinkles/icon-links/issues"
  },
  "homepage": "https://github.com/spsprinkles/icon-links#readme",
  "dependencies": {
    "datatables.net": "^2.3.2",
    "datatables.net-bs5": "^2.3.2",
    "dattatable": "^2.11.43",
    "gd-sprest-bs": "^10.14.4",
    "jquery": "^3.7.1",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "html-loader": "^4.2.0",
    "i": "^0.3.7",
    "npm": "^9.7.1",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  }
}